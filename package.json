{
  "name": "auq-mcp-server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/server.ts",
    "dev": "fastmcp dev src/server.ts",
    "lint": "prettier --check . && eslint . && tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write . && eslint --fix ."
  },
  "keywords": [
    "fastmcp",
    "mcp",
    "ask-user-questions",
    "tui",
    "terminal"
  ],
  "repository": {
    "url": "https://github.com/paulp-o/ask-user-question-mcp"
  },
  "author": "Paul Park",
  "homepage": "https://github.com/paulp-o/ask-user-question-mcp",
  "type": "module",
  "license": "MIT",
  "description": "An MCP server that provides a tool to ask a user questions via the terminal",
  "dependencies": {
    "fastmcp": "^1.27.3",
    "zod": "^3.24.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node22": "^22.0.1",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-perfectionist": "^4.12.3",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  }
}
